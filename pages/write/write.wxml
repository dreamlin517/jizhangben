<view class='write'>
  <view class='top'>
    <view class='option'>
      <button class='payment {{type==="payment"? "hightlight" : ""}}' bindtap='handleSwitchType' data-hi='payment'>支出</button>
      <button class='income {{type==="income"? "hightlight" : ""}}' bindtap='handleSwitchType' data-hi='income'>收入</button>
    </view>
    <view class='money'>
      <image class='eat-icon' src="../../images/icon_sanjiaoxing.png"></image>
      <text class='chihe'>吃喝</text>
      <text class='mon'>0.00</text>
    </view>
  </view>
  <view class='middle'>
    <view class='icons' wx:for="{{typeList}}" wx:key="{{index}}">
      <!-- <image class='select' src="../../images/icon_sanjiaoxing.png"></image> -->
      <view > {{item}} </view>
    </view>
  </view>
  <view class='buttom'>
    <view class='botton-top'>
      <button class='zhanghh'>账户</button>
      <button class='beizhu'>备注</button>
    </view>
    <view class='bottom-bottom'>
      <view class='number'>
        <view class='sever'>7</view>
        <view class='sever'>8</view>
        <view class='number-one'>9</view>
        <view class='sever'>6</view>
        <view class='sever'>5</view>
        <view class='number-one'>4</view>
        <view class='sever'>3</view>
        <view class='sever'>2</view>
        <view class='number-one'>1</view>
        <view class='sever'>0</view>
        <view class='sever'>.</view>
        <view class='number-one'>清空</view>
      </view>
      <view class='caozuo'>
        <view class='delete'>删除</view>
        <view class='sure'>确定</view>
      </view>
    </view>
  </view>
</view>